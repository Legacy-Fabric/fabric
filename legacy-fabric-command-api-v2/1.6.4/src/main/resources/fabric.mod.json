      "net.legacyfabric.fabric.impl.command.ImplInit"
    ],
    "server": [
      "net.legacyfabric.fabric.impl.command.ImplInit"
    ]
  },
  "description": "Powerful Command API that uses SpongeAPI's command api",
  "mixins": [],
  "accessWidener": "command-api-v2.accesswidener",
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "parent": "legacy-fabric-api"
    }
  }
